  <p>Type: <input type="text" id="ship_type_1" name="pirate[ships][][type]"></p>
  <p>Booty: <input type="text" id="ship_booty_1" name="pirate[ships][][booty]"></p>
  <label for="ship_2">Ship 2</label>
  <p>Name: <input type="text" id="ship_name_2" name="pirate[ships][][name]"></p>
  <p>Type: <input type="text" id="ship_type_2" name="pirate[ships][][type]"></p>
  <p>Booty: <input type="text" id="ship_booty_2" name="pirate[ships][][booty]"></p>
  <input type="submit" value="Submit" id="Submit">
</form>
